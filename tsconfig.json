{
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "types": ["node","vite/client"],
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
